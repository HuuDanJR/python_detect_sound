<!-- Content Header (Page header) -->
<section class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-12" id="content-header">
      <span class="col-md-3">
      </span>
        <h1 class="m-0 text-dark col-md-6">
          <%= t('logs') %>
        </h1>
        <ol class="breadcrumb float-sm-right col-md-3">
          <li class="breadcrumb-item"><a href="#">Home</a></li>
          <li class="breadcrumb-item active">
            <%= t('logs') %>
          </li>
        </ol>
      </div>

    </div>
  </div>
</section>
<!-- /.content-header -->

<!-- Main content -->
<section class="content" id="dashboard">
  <div class="row">
    <div class="col-md-6">
      <div class="card card-dashboard">
        <div class="card-header">
          <span class="card-title-dashboard">
            JJBX OCR
          </span>
        </div>
        <div class="card-body">
          <div class="row">
            Last Time: <%= @jjbx_machine.created_at %><br />
            <%= @jjbx_machine.grand_name %>:  <%= @jjbx_machine.grand_value %> <br />
            
            <%= @jjbx_machine.major_name %>:  <%= @jjbx_machine.major_value %> <br />

          </div>
        </div>
      </div>
    </div>
    
    <div class="col-md-6">
      <div class="card card-dashboard">
        <div class="card-header">
          <span class="card-title-dashboard">
            Mystery Jackpot
          </span>
        </div>
        <div class="card-body">
          <div class="row">
            Last Time: <%= @mystery_jackpots.created_at %> <br />
              <%= @mystery_jackpots.as_json %> <br />

          </div>
        </div>
      </div>
    </div>
    
    <div class="col-md-6">
      <div class="card card-dashboard">
        <div class="card-header">
          <span class="card-title-dashboard">
            Jackpot Machines
          </span>
        </div>
        <div class="card-body">
          <div class="row">
            Last Time: <%= @jackpot_machines.created_at %> <br />
              <%= @jackpot_machines.as_json %> <br />

          </div>
        </div>
      </div>
    </div>

     <div class="col-md-6">
      <div class="card card-dashboard">
        <div class="card-header">
          <span class="card-title-dashboard">
            Jackpot Realtime
          </span>
        </div>
        <div class="card-body">
          <div class="row">
            Last Time: <%= @jackpot_real_times.created_at %> <br />
              <%= @jackpot_real_times.data.as_json %> <br />

          </div>
        </div>
      </div>
    </div>
  </div>

</section>

<script type="text/javascript">
  $( document ).ready(function() {
    
  });

</script>